{
  "name": "phoenix-image",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "start-server": "cd src/server && node server.js",
    "start-client": "cd src/client && npm start",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "cd src/server && nodemon server.js",
    "dev-client": "cd src/client && npm run dev",
    "build": "cd src/server && npm run build",
    "lint": "cd src/server && npm run lint"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "multer": "^1.4.5-lts.1",
    "next": "14.0.1",
    "nodemon": "^3.0.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "description": "Content-Based Image Retrieval System",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "keywords": [],
  "author": "Bjir Anak Nopal",
  "license": "ISC"
}